
Public Class Cliente
    Inherits Ente
    Private vNombre As String
    Private vIdIva As Integer
    Private vIdCliente As Integer
    Private vCredito As Double

    Public Property Nombre() As String
        Get
            Return vNombre
        End Get
        Set(ByVal value As String)
            vNombre = value
        End Set
    End Property
    Public Property IdIva() As Integer
        Get
            Return vIdIva
        End Get
        Set(ByVal value As Integer)
            vIdIva = value
        End Set
    End Property
    Public Property IdCliente() As Integer
        Get
            Return vIdCliente
        End Get
        Set(ByVal value As Integer)
            vIdCliente = value
        End Set
    End Property
    Public Property Credito() As Double
        Get
            Return vCredito
        End Get
        Set(ByVal value As Double)
            vCredito = value
        End Set
    End Property
End Class
