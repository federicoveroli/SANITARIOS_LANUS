Public Class Compra
    Inherits Comprobante

    Private vIdCompra As Integer
    Private vIdProveedor As Integer
    Public Property IdCompra() As Integer
        Get
            Return vIdCompra
        End Get
        Set(ByVal value As Integer)
            vIdCompra = value
        End Set
    End Property

    Public Property IdProveedor() As Integer
        Get
            Return vIdProveedor
        End Get
        Set(ByVal value As Integer)
            vIdProveedor = value
        End Set
    End Property
End Class
