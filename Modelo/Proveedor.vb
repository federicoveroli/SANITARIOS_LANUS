Public Class Proveedor
    Inherits Ente

    Private vIdProveedor As Integer
    Private vRazonSocial As String
    Private vLista As String
    Private vBonificaciones As String
    Private vEmail As String
    Public Property IdProveedor() As Integer
        Get
            Return vIdProveedor
        End Get
        Set(ByVal value As Integer)
            vIdProveedor = value
        End Set
    End Property


    Public Property RazonSocial() As String
        Get
            Return vRazonSocial
        End Get
        Set(ByVal value As String)
            vRazonSocial = value
        End Set
    End Property

    Public Property Lista() As String
        Get
            Return vLista
        End Get
        Set(ByVal value As String)
            vLista = value
        End Set
    End Property

    Public Property Bonificaciones() As String
        Get
            Return vBonificaciones
        End Get
        Set(ByVal value As String)
            vBonificaciones = value
        End Set
    End Property

    Public Property Email() As String
        Get
            Return vEmail
        End Get
        Set(ByVal value As String)
            vEmail = value
        End Set
    End Property
End Class
