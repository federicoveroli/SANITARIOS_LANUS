Public Class Pago
    Private vNumFac As String
    Private vFecha As Date
    Dim vPago As Double
    Public Property NumFac() As Integer
        Get
            Return vNumFac
        End Get
        Set(ByVal value As Integer)
            vNumFac = value
        End Set
    End Property

    Public Property Fecha() As Date
        Get
            Return vFecha
        End Get
        Set(ByVal value As Date)
            vFecha = value
        End Set
    End Property
    Public Property Pago() As Double
        Get
            Return vPago
        End Get
        Set(ByVal value As Double)
            vPago = value
        End Set
    End Property
End Class
